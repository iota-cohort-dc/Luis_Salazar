<h1><%= @dojo.branch %></h1> <a href="/dojos/<%= @dojo.id %>/edit">Edit</a>
<br>
<p>Address: <%= @dojo.street %></p>
<p>City: <%= @dojo.city %>  State: <%= @dojo.state %></p>
<br>
<h3>Student Roster</h3>
	<table style="text-align: left; border: 2px solid black; width: 800px;">
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Actions</th>
		</tr>
		<% @students.each do |student| %>
		<tr>
			<td><%= student.first_name %></td>
			<td><%= student.last_name %></td>
			<td><%= student.email %></td>
			<td><a href="/dojos/<%= student.dojo.id %>/students/<%= student.id %>">Show</a> | <a href="/dojos/<%= student.dojo.id %>/students/<%= student.id %>/edit">Edit</a> | <a href="/students/<%= student.id %>/<%= student.dojo.id %>" data-method="delete">Destroy</a></td>
		</tr>
		<% end %>
	</table>
<br>
<a href="/dojos">Home</a> | <a href="/dojos/<%= @dojo.id %>/students/new">Add a Student</a>